{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "outDir": "",
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/*"
  ]
}
